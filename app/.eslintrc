{
    "settings": {
        "import/resolver": {
            "babel-module": {
                "root": ["./src"]
            }
        }
    },
    "extends":[
        "react-app",
        "plugin:import/warnings"
    ],
    "rules": {
        "arrow-spacing":"warn",
        "jsx-quotes": ["warn", "prefer-single"],
        "no-undef": "error",
    }
}